import easyocr
import cv2
from matplotlib import pyplot as plt
import numpy as np

# Resim dosyasının yolu
IMAGE_PATH = 'ingredients.png'

# OCR okuyucuyu başlat
reader = easyocr.Reader(['en'], gpu=False)

# Resimdeki metni oku
result = reader.readtext(IMAGE_PATH)

# Resmi OpenCV ile yükle
img = cv2.imread(IMAGE_PATH)

# Tespit edilen metinleri saklamak için bir değişken
detected_texts = []

# Her bir tespiti işleyelim
for detection in result:
    top_left = tuple([int(val) for val in detection[0][0]])
    bottom_right = tuple([int(val) for val in detection[0][2]])
    text = detection[1]
    font = cv2.FONT_HERSHEY_SIMPLEX
    # Dikdörtgen çiz ve metni ekle
    img = cv2.rectangle(img, top_left, bottom_right, (0, 255, 0), 5)
    img = cv2.putText(img, text, top_left, font, 2, (255, 255, 255), 2, cv2.LINE_AA)
    detected_texts.append(text)  # Metinleri listeye ekle
    print(text)  # Her metin parçasını yazdır

# Görüntüyü matplotlib ile göster
plt.figure(figsize=(10, 10))
plt.imshow(cv2.cvtColor(img, cv2.COLOR_BGR2RGB))
plt.show()

# Tespit edilen metinleri birleştir
all_detected_text = "\n".join(detected_texts)

