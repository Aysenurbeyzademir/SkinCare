yas_araligi_icerikler = {
    "0-13": {
        "zararlı": ["Paraben", "Sodium Lauryl Sulfate", "Fragrance", "Alcohol"],
        "yararlı": ["Aloe Vera", "Chamomile", "Oat Extract", "Shea Butter"]
    },
    "14-21": {
        "zararlı": ["Alcohol", "Fragrance", "Benzoyl Peroxide", "Sodium Lauryl Sulfate"],
        "yararlı": ["Salicylic Acid", "Niacinamide", "Tea Tree Oil", "Hyaluronic Acid"]
    },
    "22-30": {
        "zararlı": ["Alcohol", "Fragrance", "Sodium Lauryl Sulfate", "Paraben"],
        "yararlı": ["Vitamin C", "Hyaluronic Acid", "Retinol", "Peptides"]
    },
    "30-60": {
        "zararlı": ["Alcohol", "Fragrance", "Sodium Lauryl Sulfate", "Paraben"],
        "yararlı": ["Retinol", "Peptides", "Vitamin C", "Hyaluronic Acid"]
    }
}

def yas_araligini_belirle(yas):
    if yas <= 13:
        return "0-13"
    elif 14 <= yas <= 21:
        return "14-21"
    elif 22 <= yas <= 30:
        return "22-30"
    elif 31 <= yas <= 60:
        return "30-60"
    else:
        return None

def içerik_sınıflandır(text, cilt_tipi, yas_araligi):
    # Tüm metni küçük harfe çevir ve boşlukları temizle
    text = text.lower()
    içerikler = text.split("\n")
    sonuçlar = {"zararlı": [], "yararlı": [], "nötr": []}
    
    zararlı_içerikler = cilt_tipi_icerikler[cilt_tipi]["zararlı"] + yas_araligi_icerikler[yas_araligi]["zararlı"]
    yararlı_içerikler = cilt_tipi_icerikler[cilt_tipi]["yararlı"] + yas_araligi_icerikler[yas_araligi]["yararlı"]
    
    for içerik in içerikler:
        içerik = içerik.strip()
        # Zararlı içerikleri kontrol et
        for zararlı in zararlı_içerikler:
            if zararlı.lower() in içerik:
                sonuçlar["zararlı"].append(zararlı)
                break
        # Yararlı içerikleri kontrol et
        for yararlı in yararlı_içerikler:
            if yararlı.lower() in içerik:
                sonuçlar["yararlı"].append(yararlı)
                break
        else:
            sonuçlar["nötr"].append(içerik)
    
    return sonuçlar

# Kullanıcıdan cilt tipini ve yaşını al
cilt_tipi = input("Cilt tipinizi girin (kuru/yağlı/normal): ").strip().lower()
yas = int(input("Yaşınızı girin: ").strip())

# Yaş aralığını belirle
yas_araligi = yas_araligini_belirle(yas)

if yas_araligi is None:
    print("Geçersiz yaş aralığı.")
else:
    # İçerikleri sınıflandır
    sınıflandırılmış_içerikler = içerik_sınıflandır(all_detected_text, cilt_tipi, yas_araligi)

    # Uyumluluk skoru hesaplama
    def uyumluluk_skoru_hesapla(sonuçlar):
        zararlı_sayısı = len(sonuçlar["zararlı"])
        yararlı_sayısı = len(sonuçlar["yararlı"])
        toplam_sayı = zararlı_sayısı + yararlı_sayısı + len(sonuçlar["nötr"])
        
        # Temel bir skor hesaplama örneği: (yararlı - zararlı) / toplam
        if toplam_sayı > 0:
            skor = (yararlı_sayısı - zararlı_sayısı) / toplam_sayı
            uyumluluk_skoru = skor * 100  # Skoru yüzdeye çevirme
        else:
            uyumluluk_skoru = 0  # İçerik yoksa skor 0
        
        return uyumluluk_skoru

    # Uyumluluk skorunu hesapla
    uyumluluk_skoru = uyumluluk_skoru_hesapla(sınıflandırılmış_içerikler)

    # Sonuçları yazdır
    print(f"Uyumluluk Skoru: {uyumluluk_skoru}%")
    print(f"Sınıflandırılmış İçerikler: {sınıflandırılmış_içerikler}")
